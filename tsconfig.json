{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "ES2020"
    ],
    "esModuleInterop": true,
    "declaration": false,
    "jsx": "react",
    "jsxFactory": "h",
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "./build"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/**/*.test.ts"
  ]
}
